{% extends template('theme/base.html.twig') %}

{% block main %}
    <form method="POST" novalidate>
        {{ form_row(form.name) }}
        {{ form_row(form.birthday) }}
        {{ form_row(form.occupation) }}
        {{ form_row(form.picture) }}
        {{ form_row(form._token) }}
        <button type="submit">Save</button>
    </form>
{% endblock %}
