{% extends template('admin/view/base.html.twig') %}

{% trans_default_domain 'EnhavoUserBundle' %}

{% block content %}
    <div id="background" {% if branding_background_image() %}style="background-image: url({{ asset(branding_background_image()) }}){% endif %}">
        <div id="loginBox">
            <div class="loginContainer">

                {% for message in data.messages %}
                    <div class="col-md-push-2 col-md-4 {{ message.type }}" id="overlayMessage" style="display: block;">
                        <div>{{ message.message }}</div>
                    </div>
                {% endfor %}

                {% if is_branding_version() %}
                    <div class="ribbon">
                        <span>{{ branding_version() }}</span>
                    </div>
                {% endif %}

                {% if is_branding() %}
                    <div class="logo">
                        {% if branding_logo() %}
                            <img src="{{ asset(branding_logo()) }}" />
                        {% else %}
                            <img src="data:image/svg+xml;base64," />
                        {% endif %}
                    </div>
                {% endif %}

                {% block form %}{% endblock %}
            </div>
            <p>
                {% if is_branding() and branding_text() %}{{ branding_text() }} <br/>{% endif %}
                {% if is_branding() and is_branding_created_by() %}
                    Created and launched by <a href="https://www.xq-web.de" target="_blank">xq-web.de</a>
                {% endif %}
            </p>
        </div>
    </div>
{% endblock %}