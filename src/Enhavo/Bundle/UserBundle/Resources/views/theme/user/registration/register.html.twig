{% extends template('theme/base.html.twig') %}
{% trans_default_domain 'EnhavoUserBundle' %}

{% block main %}
    <form action="{{ path('enhavo_user_theme_registration_register') }}" method="POST">
        {{ form_row(form.email) }}
        {{ form_row(form.plainPassword) }}
        {{ form_row(form._token) }}
        <button type="submit">{{ 'registration.form.submit'|trans }}</button>
    </form>
{% endblock %}
