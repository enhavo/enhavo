    <h1>Confirm</h1>
    <div>
        <form action="{{ path('sylius_shop_checkout_confirm') }}" method="POST" data-checkout-form>
            {{ form_errors(form) }}
            {{ form_row(form._token) }}
            <button type="submit">Confirm</button>
        </form>
    </div>
{% endblock %}
