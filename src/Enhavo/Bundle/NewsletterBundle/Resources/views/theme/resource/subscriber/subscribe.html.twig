<div class='subscribe'>
    <form method="post" action="{{ path('enhavo_newsletter_subscribe_add', { 'type': 'default' }) }}" data-newsletter-subscribe-form>
        {{ form_widget(form._token) }}
        {{ form_widget(form.email) }}
        <button type="submit" id="addEmail" class="button">Add email</button>
    </form>
</div>