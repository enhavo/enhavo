<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping">
    <entity name="Enhavo\Bundle\SearchBundle\Model\Database\Total" table="search_total" repository-class="Enhavo\Bundle\SearchBundle\Repository\TotalRepository">
        <id name="id" type="integer" column="id">
            <generator />
        </id>

        <field name="word" length="255" />
        <field name="count" type="float" />
    </entity>
</doctrine-mapping>
